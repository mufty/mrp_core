{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
			"."
		],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules|docs)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/jsdoc-baseline/",
		"destination": "fivem-mrp.github.io",
		"recurse": true
	}
}